@model IEnumerable<DIUB.Models.Tbl_Notes>

@{
    ViewBag.Title = "SeeNotes";
    Layout = "~/Views/Shared/_StudentLayout.cshtml";
}

<h2>SeeNotes</h2>

<hr />
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.Note)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Note)
        </td>
       
       
        <td>
            @Html.ActionLink("Download", "Download", new {  picture=item.Note }) |
            
        </td>
    </tr>
}

</table>
